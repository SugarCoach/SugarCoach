<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/constraintLayout">

        <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:srcCompat="@drawable/space_bg"
                android:id="@+id/bg"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toTopOf="@+id/guideline"/>

        <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/register_date_txt"
                style="@style/TextStyle.Date"
                android:layout_width="80dp"
                android:layout_height="35dp"
                android:background="@drawable/date_right_bg"
                android:gravity="center"
                android:textSize="14dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/register_time_txt"
                style="@style/TextStyle.Date"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:background="@drawable/date_left_bg"
                android:gravity="center"
                android:minWidth="80dp"
                android:paddingLeft="5dp"
                android:textSize="14dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


        <FrameLayout
                android:id="@+id/frameLayout2"
                android:layout_width="match_parent"
                android:layout_height="240dp"
                android:layout_marginTop="20dp"
                app:layout_constraintBottom_toBottomOf="@+id/guideline"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            <ImageView
                    android:id="@+id/register_planet_before"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="10dp"
                    android:scaleType="centerCrop"
                    android:visibility="gone"
                    app:srcCompat="@drawable/planet_glucemia" />

            <ImageView
                    android:id="@+id/register_planet"
                    android:layout_width="240dp"
                    android:layout_height="240dp"
                    android:layout_gravity="center"
                    android:visibility="visible"
                    app:srcCompat="@drawable/planet_glucemia" />

            <ImageView
                    android:id="@+id/register_planet_after"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_gravity="right|center_vertical"
                    android:layout_marginEnd="10dp"
                    android:scaleType="centerCrop"
                    android:visibility="visible"
                    app:srcCompat="@drawable/planet_insulina" />
        </FrameLayout>

        <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="@+id/bg"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/register_userimg_iv">

            <TextView
                    android:id="@+id/register_type"
                    style="@style/TextStyle.Heading"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rectangle_white"
                    android:gravity="center"
                    android:padding="5dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="@string/main_glucemia_txt"
                    android:textColor="@color/aqua"
                    android:textSize="16dp" />

            <com.mazenrashed.dotsindicator.DotsIndicator
                    android:id="@+id/register_dots"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="0dp"
                    app:dot_height="10dp"
                    app:dot_width="10dp"
                    app:dots_count="5"
                    app:first_dot_height="10dp"
                    app:first_dot_width="10dp"
                    app:first_selected_dot_resource="@drawable/circle_pink"
                    app:first_unselected_dot_resource="@drawable/circle_gray"
                    app:margins_between_dots="17dp"
                    app:selected_dot_resource="@drawable/circle_pink"
                    app:selected_dot_scale_factor="1"
                    app:unselected_dot_resource="@drawable/circle_gray" />

        </LinearLayout>

        <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                app:cardBackgroundColor="@color/pink"
                app:cardCornerRadius="10dp"
                app:cardUseCompatPadding="true"
                app:cardElevation="2dp"
                app:cardPreventCornerOverlap="true"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toEndOf="@+id/register_date_txt"
                android:layout_marginStart="40dp"
                android:layout_marginEnd="40dp"
                app:layout_constraintEnd_toStartOf="@+id/register_time_txt"
                app:layout_constraintTop_toBottomOf="@+id/register_date_txt"
                android:id="@+id/cardView3">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp">
                <androidx.cardview.widget.CardView
                        app:cardElevation="0dp"
                        app:cardCornerRadius="10dp"
                        android:layout_marginTop="5dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">
                    <com.skydoves.powerspinner.PowerSpinnerView
                            android:layout_width="match_parent"
                            android:layout_height="35dp"
                            android:background="@color/white"
                            android:gravity="center"
                            android:padding="5dp"
                            android:textColor="@color/black"
                            android:textColorHint="@color/black"
                            android:textSize="14dp"
                            android:hint="Seleccione una"
                            app:spinner_arrow_gravity="end"
                            app:spinner_arrow_tint="@color/pink"
                            app:spinner_arrow_padding="8dp"
                            app:spinner_divider_color="@color/gray"
                            app:spinner_divider_show="true"
                            app:spinner_divider_size="0.4dp"
                            app:fontFamily="@font/folks_normal"
                            app:spinner_popup_animation="dropdown"
                            app:spinner_popup_background="@color/white"
                            app:spinner_popup_elevation="14dp"
                            android:id="@+id/register_med_txt"/>
                </androidx.cardview.widget.CardView>
                <androidx.appcompat.widget.AppCompatTextView
                        android:text="@string/main_prox_med_txt"
                        style="@style/TextStyle.Title"
                        android:layout_gravity="center"
                        android:layout_width="match_parent"
                        android:gravity="center"
                        android:layout_height="20dp"
                        android:textSize="14dp"/>

            </LinearLayout>
        </androidx.cardview.widget.CardView>
        <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/guideline"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.50"/>

        <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_user_nophoto"
                app:civ_border_color="@color/white"
                app:civ_border_width="2dp"
                android:id="@+id/register_userimg_iv"
                app:layout_constraintBottom_toTopOf="@+id/register_username_txt"
                app:layout_constraintStart_toStartOf="@+id/register_username_txt"
                app:layout_constraintEnd_toEndOf="@+id/register_username_txt"/>
        <androidx.appcompat.widget.AppCompatTextView
                style="@style/TextStyle.Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="32dp"
                android:layout_marginBottom="8dp"
                android:id="@+id/register_username_txt"
                app:layout_constraintBottom_toTopOf="@+id/guideline"/>
        <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="0dp"
                app:layout_constraintTop_toBottomOf="@+id/bg"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="1.0">
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                <LinearLayout
                        android:id="@+id/register_comentario"
                        android:layout_width="67dp"
                        android:layout_height="0dp"
                        android:layout_marginEnd="24dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="gone"
                        app:layout_constraintBottom_toTopOf="@+id/register_completed_iv"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/register_next_iv">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_comment" />

                    <TextView
                            android:id="@+id/register_comentario_tv"
                            style="@style/TextStyle.Actions"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLines="2"
                            android:text="@string/register_add_comment"
                            android:textColor="@color/blue" />
                </LinearLayout>

                <LinearLayout
                        android:id="@+id/register_img_ly"
                        android:layout_width="67dp"
                        android:layout_height="0dp"
                        android:layout_marginEnd="24dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:visibility="gone"
                        app:layout_constraintBottom_toTopOf="@+id/register_completed_iv"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/register_next_iv">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:id="@+id/register_img_iv"
                            android:layout_width="45dp"
                            android:layout_height="45dp"
                            android:layout_gravity="center"
                            android:src="@drawable/ic_add_photo" />

                    <TextView
                            style="@style/TextStyle.Actions"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLines="2"
                            android:text="@string/register_add_photo"
                            android:textColor="@color/blue" />
                </LinearLayout>

                <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/register_ptsText_txt"
                        style="@style/TextStyle.Heading"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="8dp"
                        android:layout_marginEnd="8dp"
                        android:layout_marginBottom="16dp"
                        android:text="@string/register_pts_txt"
                        android:textColor="@color/blue"
                        android:textSize="12dp"
                        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />
                <androidx.cardview.widget.CardView
                        android:orientation="vertical"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        app:cardBackgroundColor="@color/purple"
                        app:cardElevation="5dp"
                        app:cardCornerRadius="10dp"
                        android:id="@+id/register_prev_iv"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="32dp"
                        android:visibility="invisible"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="24dp">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_height="match_parent">

                        <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                app:srcCompat="@drawable/icon_prev" />

                        <TextView
                                android:text="@string/register_prev_txt"
                                android:layout_width="wrap_content"
                                style="@style/TextStyle.Actions"
                                android:layout_height="wrap_content"
                                android:textSize="12dp"
                                android:textColor="@color/white"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:orientation="vertical"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        app:cardBackgroundColor="@color/purple"
                        app:cardElevation="5dp"
                        app:cardCornerRadius="10dp"
                        android:id="@+id/register_again_iv"
                        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
                        android:layout_marginBottom="32dp"
                        android:visibility="visible"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginStart="24dp">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_height="match_parent">

                        <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                app:srcCompat="@drawable/icon_again" />

                        <TextView
                                android:text="@string/register_again_txt"
                                android:layout_width="wrap_content"
                                style="@style/TextStyle.Actions"
                                android:layout_height="wrap_content"
                                android:textSize="12dp"
                                android:textColor="@color/white"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:orientation="vertical"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        app:cardBackgroundColor="@color/purple"
                        app:cardElevation="5dp"
                        app:cardCornerRadius="10dp"
                        android:id="@+id/register_next_iv"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="32dp"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginEnd="24dp">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_height="match_parent">

                        <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                app:srcCompat="@drawable/icon_next" />

                        <TextView
                                android:text="@string/register_next_txt"
                                android:layout_width="wrap_content"
                                style="@style/TextStyle.Actions"
                                android:layout_height="wrap_content"
                                android:textSize="12dp"
                                android:textColor="@color/white"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                        android:orientation="vertical"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        app:cardBackgroundColor="@color/purple"
                        app:cardElevation="5dp"
                        app:cardCornerRadius="10dp"
                        android:id="@+id/register_completed_iv"
                        app:layout_constraintBottom_toTopOf="@+id/linearLayout"
                        android:layout_marginBottom="32dp"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_marginEnd="24dp">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_height="match_parent">

                        <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                app:srcCompat="@drawable/icon_finish" />

                        <TextView
                                android:text="@string/register_completed_txt"
                                android:layout_width="wrap_content"
                                style="@style/TextStyle.Actions"
                                android:layout_height="wrap_content"
                                android:textSize="12dp"
                                android:textColor="@color/white"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="64dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:id="@+id/linearLayout"
                        android:background="@color/aqua">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:id="@+id/home"
                            android:gravity="center"
                            android:layout_height="match_parent"
                            android:layout_weight="0.20">
                        <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="43dp"
                                android:layout_height="43dp"
                                android:layout_gravity="center"
                                android:scaleType="centerCrop"
                                app:srcCompat="@drawable/ic_profile"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:id="@+id/statistics"
                            android:layout_height="match_parent"
                            android:layout_weight="0.20">
                        <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="43dp"
                                android:layout_height="43dp"
                                android:layout_gravity="center"
                                android:scaleType="centerCrop"
                                app:srcCompat="@drawable/ic_statistics"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.20">
                        <FrameLayout
                                android:layout_width="match_parent"
                                android:layout_height="43dp">
                            <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    app:srcCompat="@drawable/bold_shape"/>
                            <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="38dp"
                                    android:layout_height="43dp"
                                    android:layout_gravity="center"
                                    android:scaleType="centerCrop"
                                    app:srcCompat="@drawable/ic_control"/>
                        </FrameLayout>

                        <TextView
                                android:text="@string/main_register_txt"
                                style="@style/TextStyle.Title"
                                android:textSize="10dp"
                                android:textStyle="bold"
                                android:layout_gravity="center"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                    </LinearLayout>
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:id="@+id/treament"
                            android:layout_height="match_parent"
                            android:layout_weight="0.20">
                        <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="43dp"
                                android:layout_height="43dp"
                                android:layout_gravity="center"
                                android:scaleType="centerCrop"
                                app:srcCompat="@drawable/ic_treatment"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:gravity="center"
                            android:id="@+id/dailyRegister"
                            android:layout_height="match_parent"
                            android:layout_weight="0.20">
                        <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="48dp"
                                android:layout_height="43dp"
                                android:layout_gravity="center"
                                android:scaleType="centerCrop"
                                app:srcCompat="@drawable/ic_add_register"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:id="@+id/frameLayout"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toTopOf="@+id/cardView2">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="190dp"
                            android:layout_height="190dp"
                            app:srcCompat="@drawable/progress_circular" />

                    <FrameLayout
                            android:layout_width="170dp"
                            android:layout_gravity="center"
                            android:layout_height="wrap_content">
                        <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="match_parent"
                                android:layout_gravity="center"
                                android:layout_height="match_parent"/>
                        <LinearLayout
                                android:layout_width="match_parent"
                                android:orientation="vertical"
                                android:gravity="center"
                                android:layout_height="match_parent">
                            <androidx.appcompat.widget.AppCompatEditText
                                    android:hint="0"
                                    style="@style/TextStyle.Heading"
                                    android:gravity="center"
                                    android:textSize="36dp"
                                    android:inputType="numberDecimal"
                                    android:digits="0123456789.,"
                                    android:imeOptions="actionDone"
                                    android:textColorHint="@color/gray"
                                    android:background="@android:color/transparent"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/register_value_txt"
                                    android:visibility="visible"/>
                            <ImageView
                                    android:layout_width="50dp"
                                    app:srcCompat="@drawable/ic_actividad"
                                    android:tint="@color/pink"
                                    android:id="@+id/register_exercise_img"
                                    android:layout_gravity="center"
                                    android:layout_height="50dp"
                                    android:visibility="gone"/>
                            <ImageView
                                    android:layout_width="50dp"
                                    app:srcCompat="@drawable/ic_emotions"
                                    android:id="@+id/register_emotional_img"
                                    android:layout_gravity="center"
                                    android:tint="@color/pink"
                                    android:layout_height="50dp"
                                    android:visibility="gone"/>
                            <androidx.appcompat.widget.AppCompatTextView
                                    android:text="@string/register_emotional_label"
                                    style="@style/TextStyle.Heading"
                                    android:layout_gravity="center"
                                    android:textSize="10dp"
                                    android:visibility="gone"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/register_emotional_tv"/>
                            <androidx.appcompat.widget.AppCompatTextView
                                    android:text="@string/register_exercises_label"
                                    style="@style/TextStyle.Heading"
                                    android:layout_gravity="center"
                                    android:textSize="10dp"
                                    android:visibility="gone"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/register_exercices_tv"/>
                            <androidx.appcompat.widget.AppCompatTextView
                                    android:text="@string/register_new_register_txt"
                                    style="@style/TextStyle.Heading"
                                    android:layout_gravity="center"
                                    android:textSize="14dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:id="@+id/register_unidad_tv"/>
                            <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="30dp"
                                    android:layout_height="40dp"
                                    android:layout_gravity="center"
                                    android:scaleType="centerCrop"
                                    app:srcCompat="@drawable/ic_hand"
                                    android:layout_marginTop="2dp"/>
                        </LinearLayout>
                    </FrameLayout>
                </FrameLayout>
                <androidx.cardview.widget.CardView
                        android:layout_width="98dp"
                        android:orientation="vertical"
                        android:layout_height="45dp"
                        app:cardBackgroundColor="@color/blue"
                        app:cardElevation="5dp"
                        app:cardCornerRadius="10dp"
                        android:id="@+id/cardView2"
                        android:layout_marginStart="8dp"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toTopOf="@+id/register_ptsText_txt">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_height="match_parent">
                        <androidx.appcompat.widget.AppCompatTextView
                                android:text="1800"
                                style="@style/TextStyle.Heading"
                                android:gravity="center"
                                android:textSize="18dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/register_pts_txt"/>
                        <androidx.appcompat.widget.AppCompatTextView
                                android:text="Nivel 7"
                                android:id="@+id/register_level_txt"
                                style="@style/TextStyle.Heading"
                                android:layout_gravity="center"
                                android:textSize="12dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>