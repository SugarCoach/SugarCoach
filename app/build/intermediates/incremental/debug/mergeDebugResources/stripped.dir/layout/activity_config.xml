<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              xmlns:tools="http://schemas.android.com/tools"
              xmlns:mask="http://schemas.android.com/apk/res-auto"  android:orientation="vertical"
              android:background="@color/white"
              android:fitsSystemWindows="true"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:id="@+id/constraintLayout">

        <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/space_bg"
                android:id="@+id/bg"
                android:scaleType="centerCrop"
                app:layout_constraintBottom_toTopOf="@+id/guideline"/>
        <androidx.constraintlayout.widget.Guideline
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/guideline"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.25"/>

        <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="54dp"
                android:layout_height="54dp"
                android:scaleType="centerCrop"
                android:src="@drawable/ic_user_nophoto"
                app:civ_border_color="@color/white"
                app:civ_border_width="2dp"
                android:id="@+id/config_userimg_iv"
                app:layout_constraintBottom_toTopOf="@+id/config_username_txt"
                app:layout_constraintStart_toStartOf="@+id/config_username_txt"
                app:layout_constraintEnd_toEndOf="@+id/config_username_txt"/>

        <androidx.appcompat.widget.AppCompatTextView
                style="@style/TextStyle.Title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="32dp"
                android:layout_marginBottom="8dp"
                android:id="@+id/config_username_txt"
                app:layout_constraintBottom_toTopOf="@+id/guideline"/>


        <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginBottom="75dp"
                app:layout_constraintTop_toBottomOf="@+id/guideline"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="16dp">
            <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:gravity="center"
                        android:id="@+id/linear"
                        android:layout_height="75dp"
                        android:layout_marginTop="8dp"
                        app:layout_constraintTop_toTopOf="parent">


                    <LinearLayout android:layout_width="0dp"
                                  android:orientation="vertical"
                                  android:paddingLeft="14dp"
                                  android:paddingRight="14dp"
                                  android:layout_marginRight="6dp"
                                  android:layout_weight="0.50"
                                  android:layout_height="match_parent"
                                  android:gravity="center"
                                  android:background="@drawable/input_border_bottom">
                        <TextView
                                android:text="@string/config_account_label"
                                style="@style/TextStyle.Heading"
                                android:layout_gravity="left"
                                android:textSize="14dp"
                                android:layout_marginTop="11dp"
                                android:gravity="center"
                                android:textColor="@color/greyConfig"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                        <TextView
                                android:id="@+id/config_type"
                                style="@style/TextStyle.Heading"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left"
                                android:layout_marginTop="6dp"
                                android:layout_marginBottom="12dp"
                                android:gravity="center"
                                android:text="@string/config_standard_label"
                                android:textColor="@color/aqua"
                                android:textSize="15dp" />
                    </LinearLayout>
                    <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            app:cardBackgroundColor="@color/pink"
                            app:cardCornerRadius="10dp"
                            android:paddingLeft="14dp"
                            android:paddingRight="14dp"
                            android:id="@+id/config_upgrade"
                            app:cardElevation="5dp"
                            android:layout_marginLeft="6dp"
                            android:layout_weight="0.50"
                            android:layout_height="match_parent"
                            app:cardPreventCornerOverlap="true">
                        <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:gravity="center"
                                android:layout_height="match_parent">

                            <TextView
                                    style="@style/TextStyle.Heading"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="0.60"
                                    android:gravity="center"
                                    android:text="@string/config_upgrade_label"
                                    android:textColor="@color/yellow"
                                    android:textSize="15dp" />
                            <FrameLayout android:layout_width="0dp"
                                         android:layout_weight="0.40"
                                         android:layout_height="match_parent">
                                <androidx.appcompat.widget.AppCompatImageView
                                        android:layout_width="match_parent"
                                        android:layout_gravity="center"
                                        android:layout_height="55dp"
                                        app:srcCompat="@drawable/ic_upgrade"/>
                            </FrameLayout>

                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                </LinearLayout>
                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:gravity="center"
                            android:paddingLeft="14dp"
                            android:layout_marginTop="14dp"
                            android:paddingRight="14dp"
                            android:background="@drawable/input_border_bottom"
                            android:layout_height="61dp">

                        <TextView
                                style="@style/TextStyle.Heading"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.50"
                                android:gravity="center_vertical"
                                android:drawableEnd="@drawable/info"
                                android:drawablePadding="4dp"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:id="@+id/config_sms_title"
                                android:text="@string/config_sms_label"
                                android:textColor="@color/blue"
                                android:textSize="12dp" />
                        <LinearLayout
                                android:layout_width="0dp"
                                android:orientation="horizontal"
                                android:layout_height="22dp"
                                android:gravity="center"
                                android:layout_weight="0.50"
                                android:layout_marginTop="5dp">

                            <androidx.appcompat.widget.AppCompatTextView
                                    style="@style/TextStyle.Heading"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="15dp"
                                    android:gravity="center"
                                    android:text="@string/treatment_no_txt"
                                    android:textAllCaps="true"
                                    android:textColor="@color/blue"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            <androidx.appcompat.widget.SwitchCompat
                                    android:id="@+id/config_sms"
                                    style="@style/CustomSwitch"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:checked="false"
                                    android:showText="false"
                                    android:enabled="false"
                                    app:thumbTint="@color/white"
                                    app:trackTint="@color/blue" />

                            <androidx.appcompat.widget.AppCompatTextView
                                    style="@style/TextStyle.Heading"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="15dp"
                                    android:gravity="center"
                                    android:text="@string/treatment_yes_txt"
                                    android:textAllCaps="true"
                                    android:textColor="@color/blue"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />
                        </LinearLayout>


                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="20dp"
                            android:layout_gravity="right"
                            android:layout_height="20dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginRight="10dp"
                            app:srcCompat="@drawable/ic_upgrade"/>
                </FrameLayout>
                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <com.vicmikhailau.maskededittext.MaskedEditText
                            android:gravity="center|center_vertical"
                            android:background="@drawable/border_aqua"
                            android:inputType="phone"
                            mask:mask="+## (###) ####-##-##"
                            android:textSize="15dp"
                            android:hint="0123456789012"
                            android:padding="5dp"
                            android:layout_marginTop="14dp"
                            android:id="@+id/config_number"
                            android:enabled="false"
                            android:imeOptions="actionDone"
                            style="@style/TextStyle.Heading"
                            android:textColorHint="@color/greyConfig"
                            android:textColor="@color/aqua"
                            android:visibility="visible"
                            android:layout_width="match_parent"
                            android:layout_height="61dp"
                    />
                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="25dp"
                            android:layout_gravity="left"
                            android:layout_height="25dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginLeft="10dp"
                            android:tint="@color/aqua"
                            android:id="@+id/config_sms_info"
                            app:srcCompat="@drawable/ic_info_blue"/>
                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="20dp"
                            android:layout_gravity="right"
                            android:layout_height="20dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginRight="10dp"
                            app:srcCompat="@drawable/ic_upgrade"/>
                </FrameLayout>
                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:minHeight="65dp"
                            android:background="@drawable/input_border_bottom"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:paddingLeft="14dp"
                            android:paddingRight="14dp">

                        <TextView
                                style="@style/TextStyle.Heading"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.50"
                                android:gravity="center_vertical"
                                android:drawableEnd="@drawable/info"
                                android:drawableTint="@color/pink"
                                android:drawablePadding="4dp"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:id="@+id/config_geo_title"
                                android:text="@string/config_geolocalizacion_label"
                                android:textColor="@color/pink"
                                android:textSize="12dp" />

                        <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="22dp"
                                android:layout_marginTop="5dp"
                                android:layout_weight="0.50"
                                android:gravity="center"
                                android:orientation="horizontal">

                            <androidx.appcompat.widget.AppCompatTextView
                                    style="@style/TextStyle.Heading"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="15dp"
                                    android:gravity="center"
                                    android:text="@string/treatment_no_txt"
                                    android:textAllCaps="true"
                                    android:textColor="@color/pink"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />

                            <androidx.appcompat.widget.SwitchCompat
                                    android:id="@+id/config_geo"
                                    style="@style/CustomSwitch"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:checked="false"
                                    android:enabled="false"
                                    android:showText="false"
                                    app:thumbTint="@color/white"
                                    app:trackTint="@color/pink" />

                            <androidx.appcompat.widget.AppCompatTextView
                                    style="@style/TextStyle.Heading"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="15dp"
                                    android:gravity="center"
                                    android:text="@string/treatment_yes_txt"
                                    android:textAllCaps="true"
                                    android:textColor="@color/pink"
                                    android:textSize="12dp"
                                    android:textStyle="bold" />
                        </LinearLayout>


                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="20dp"
                            android:layout_gravity="right"
                            android:layout_height="20dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginRight="10dp"
                            app:srcCompat="@drawable/ic_upgrade"/>
                </FrameLayout>
                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingLeft="14dp"
                            android:layout_marginTop="14dp"
                            android:paddingRight="14dp"
                            android:background="@drawable/input_border_bottom"
                            android:layout_height="wrap_content">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:gravity="center"
                            android:id="@+id/config_control_linear"
                            android:visibility="visible"
                            android:layout_marginTop="2dp"
                            android:background="@drawable/border_buttom"
                            android:layout_height="61dp">
                        <TextView
                                style="@style/TextStyle.Heading"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:textColor="@color/yellow"
                                android:layout_width="0dp"
                                android:layout_weight="0.50"
                                android:drawableEnd="@drawable/info"
                                android:drawableTint="@color/yellow"
                                android:drawablePadding="4dp"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:id="@+id/config_control_title"
                                android:layout_height="match_parent"
                                android:text="@string/config_control_label"/>
                        <LinearLayout
                                android:layout_width="0dp"
                                android:orientation="horizontal"
                                android:layout_height="22dp"
                                android:gravity="center"
                                android:layout_weight="0.50"
                                android:layout_marginTop="5dp">

                            <androidx.appcompat.widget.AppCompatTextView
                                    style="@style/TextStyle.Heading"
                                    android:gravity="center"
                                    android:textSize="12dp"
                                    android:textColor="@color/yellow"
                                    android:layout_marginRight="15dp"
                                    android:layout_width="wrap_content"
                                    android:textAllCaps="true"
                                    android:textStyle="bold"
                                    android:layout_height="match_parent"
                                    android:text="@string/treatment_no_txt"/>
                            <androidx.appcompat.widget.SwitchCompat
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:id="@+id/config_control"
                                    android:showText="false"
                                    android:enabled="false"
                                    style="@style/CustomSwitch"
                                    app:thumbTint="@color/white"
                                    app:trackTint="@color/yellow"
                                    android:checked="false"/>
                            <androidx.appcompat.widget.AppCompatTextView
                                    style="@style/TextStyle.Heading"
                                    android:gravity="center"
                                    android:textSize="12dp"
                                    android:layout_marginLeft="15dp"
                                    android:textColor="@color/yellow"
                                    android:textAllCaps="true"
                                    android:textStyle="bold"
                                    android:enabled="false"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="@string/treatment_yes_txt"/>
                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:gravity="center"
                            android:layout_marginBottom="2dp"
                            android:visibility="visible"
                            android:layout_height="61dp" >
                        <TextView
                                style="@style/TextStyle.Heading"
                                android:gravity="center_vertical"
                                android:textSize="12dp"
                                android:textColor="@color/yellow"
                                android:layout_width="0dp"
                                android:layout_weight="0.50"
                                android:paddingLeft="5dp"
                                android:paddingRight="5dp"
                                android:layout_height="match_parent"
                                android:text="@string/config_qr_label"/>
                        <LinearLayout
                                android:layout_width="0dp"
                                android:orientation="horizontal"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:id="@+id/config_control_qr"
                                android:layout_weight="0.50"
                                android:layout_marginTop="5dp">

                            <androidx.appcompat.widget.AppCompatImageView
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    app:srcCompat="@drawable/ic_qr"/>
                        </LinearLayout>


                    </LinearLayout>
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatImageView
                                android:layout_width="20dp"
                                android:layout_gravity="right"
                                android:layout_height="20dp"
                                android:layout_marginTop="18dp"
                                android:layout_marginRight="10dp"
                                app:srcCompat="@drawable/ic_upgrade"/>
                </FrameLayout>
                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                    <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:paddingLeft="14dp"
                            android:layout_marginTop="14dp"
                            android:paddingRight="14dp"
                            android:background="@drawable/input_border_bottom"
                            android:layout_height="wrap_content">
                        <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:gravity="center"
                                android:id="@+id/config_medico_linear"
                                android:visibility="visible"
                                android:layout_marginTop="2dp"
                                android:background="@drawable/border_buttom"
                                android:layout_height="61dp">
                            <TextView
                                    style="@style/TextStyle.Heading"
                                    android:gravity="center_vertical"
                                    android:textSize="12dp"
                                    android:textColor="@color/purple"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.50"
                                    android:drawableEnd="@drawable/info"
                                    android:drawableTint="@color/purple"
                                    android:drawablePadding="4dp"
                                    android:paddingLeft="5dp"
                                    android:paddingRight="5dp"
                                    android:id="@+id/config_medico_title"
                                    android:layout_height="match_parent"
                                    android:text="@string/config_medico_label"/>
                            <LinearLayout
                                    android:layout_width="0dp"
                                    android:orientation="horizontal"
                                    android:layout_height="22dp"
                                    android:gravity="center"
                                    android:layout_weight="0.50"
                                    android:layout_marginTop="5dp">

                                <androidx.appcompat.widget.AppCompatTextView
                                        style="@style/TextStyle.Heading"
                                        android:gravity="center"
                                        android:textSize="12dp"
                                        android:textColor="@color/purple"
                                        android:layout_marginRight="15dp"
                                        android:layout_width="wrap_content"
                                        android:textAllCaps="true"
                                        android:textStyle="bold"
                                        android:layout_height="match_parent"
                                        android:text="@string/treatment_no_txt"/>
                                <androidx.appcompat.widget.SwitchCompat
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:id="@+id/config_medico"
                                        android:showText="false"
                                        android:enabled="false"
                                        style="@style/CustomSwitch"
                                        app:thumbTint="@color/white"
                                        app:trackTint="@color/purple"
                                        android:checked="false"/>
                                <androidx.appcompat.widget.AppCompatTextView
                                        style="@style/TextStyle.Heading"
                                        android:gravity="center"
                                        android:textSize="12dp"
                                        android:layout_marginLeft="15dp"
                                        android:textColor="@color/purple"
                                        android:textAllCaps="true"
                                        android:textStyle="bold"
                                        android:enabled="false"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:text="@string/treatment_yes_txt"/>
                            </LinearLayout>

                        </LinearLayout>
                        <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:gravity="center"
                                android:layout_marginBottom="2dp"
                                android:visibility="visible"
                                android:layout_height="61dp" >
                            <TextView
                                    style="@style/TextStyle.Heading"
                                    android:gravity="center_vertical"
                                    android:textSize="12dp"
                                    android:textColor="@color/purple"
                                    android:layout_width="0dp"
                                    android:layout_weight="0.50"
                                    android:paddingLeft="5dp"
                                    android:paddingRight="5dp"
                                    android:layout_height="match_parent"
                                    android:text="@string/config_qr_label"/>
                            <LinearLayout
                                    android:layout_width="0dp"
                                    android:orientation="horizontal"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:id="@+id/config_medico_qr"
                                    android:layout_weight="0.50"
                                    android:layout_marginTop="5dp">

                                <androidx.appcompat.widget.AppCompatImageView
                                        android:layout_width="30dp"
                                        android:layout_height="30dp"
                                        app:srcCompat="@drawable/ic_qr"/>
                            </LinearLayout>


                        </LinearLayout>

                        <Button
                                android:id="@+id/privacy_policy_button"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="PolÃ­tica de Privacidad" />
                    </LinearLayout>
                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="20dp"
                            android:layout_gravity="right"
                            android:layout_height="20dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginRight="10dp"
                            app:srcCompat="@drawable/ic_upgrade"/>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatTextView
                        style="@style/TextStyle.Heading"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:layout_marginBottom="14dp"
                        android:layout_marginTop="14dp"
                        android:textSize="20dp"
                        android:textColor="@color/aqua"
                        android:gravity="center"
                        android:text="@string/config_preguntas_label"
                        android:id="@+id/config_questions"
                        app:layout_constraintBottom_toTopOf="@+id/guideline"/>
            </LinearLayout>

        </androidx.core.widget.NestedScrollView>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="54dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:id="@+id/linearLayout"
                android:background="@color/aqua">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:gravity="center"
                    android:id="@+id/home"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20">
                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/ic_profile"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:gravity="center"
                    android:id="@+id/statistics"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20">
                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/ic_statistics"/>
            </LinearLayout>

            <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20"
                    android:orientation="vertical">

                <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="33dp">

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:srcCompat="@drawable/bold_shape" />

                    <androidx.appcompat.widget.AppCompatImageView
                            android:layout_width="33dp"
                            android:layout_height="33dp"
                            android:layout_gravity="center"
                            android:scaleType="fitCenter"
                            app:srcCompat="@drawable/ic_config_aqua" />
                </FrameLayout>

                <TextView
                        style="@style/TextStyle.Title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom|center"
                        android:gravity="center"
                        android:text="@string/main_config_txt"
                        android:textSize="10dp"
                        android:textStyle="bold" />

            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:gravity="center"
                    android:id="@+id/treament"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20">
                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/ic_treatment"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:id="@+id/dailyRegister"
                    android:gravity="center"
                    android:layout_height="match_parent"
                    android:layout_weight="0.20">
                <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="33dp"
                        android:layout_height="33dp"
                        android:layout_gravity="center"
                        android:scaleType="centerCrop"
                        app:srcCompat="@drawable/ic_add_register"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>